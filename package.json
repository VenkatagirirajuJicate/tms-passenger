{
  "name": "tms-passenger",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3003",
    "dev:turbo": "next dev --port 3003 --turbopack",
    "build": "next build",
    "start": "next start --port 3003",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
